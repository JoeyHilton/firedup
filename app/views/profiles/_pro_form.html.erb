<div class=container>
  <div>
  <%= bootstrap_form_for @profile do |f| %>

    <%= f.text_field :up_fname, label: "First Name" %>

    <%= f.text_field :up_lname, label: "Last Name" %>

    <%= f.text_field :up_address, label: "Address" %>

    <%= f.text_field :up_city, label: "City" %>

    <%= f.text_field :up_state, label: "State" %>

    <%= f.number_field :up_zip, label: "ZIP" %>

    <%= f.text_field :up_twitter, label: "Twitter" %>

    <%= f.date_field :up_birthdate, label: "Birthdate" %>

    <%= f.number_field :up_phone, label: "Phone" %>

    <%= f.number_field :up_mobilephone, label: "Mobile Phone" %>

    <%= f.select :up_gender, Profile::GENDER, prompt: "Pick one", label: "Gender" %>

    <%= f.text_field :up_secondemail, label: "Secondary Email" %>

    <%= f.text_area :up_bio, label: "Bio", cols: 10, rows: 7 %>

    <%= f.submit %>
  <% end %>
  </div>
</div>
