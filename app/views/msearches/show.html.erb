<div class="container">

  <div class="container"><!-- START HEADER CONTAINER -->
    <div class="row">
          <div class="col-md-12 text-center headline3">
            <h2>Members</h2>
          </div>
    </div>
  </div><!-- END HEADER CONTAINER -->

  <div class="container"><!-- START Body CONTAINER -->
  <div class="row"><!-- Start Second Row -->

    <div class="col-md-2"><!-- Start Left Column -->


      <h3>Place Holder</h3>
 

    </div><!-- End Left Column -->

    <div class="col-md-6"><!-- Start Center Column -->

        <div class="panel panel-default">
          <div class="center panel-heading">      
            <h1> Search Results </h1>
            <%= link_to 'Search Again', users_path(), class: "btn btn-primary" %>
          </div>
          
          <div class="panel-body">
            
            <% if @msearch.search_members.empty? %>
                <p>No Records Found</p>
            <% else %> 
                
              <% @msearch.search_members.order("created_at DESC").each do |member| %>

                  <strong><h4 class="jboardTitleOverride"><%= link_to "#{member.first_name} #{member.last_name}", user_path(member) %></h4></strong>

                    <%= "#{member.city}, #{member.state}" %><br>

                    <small class="text-muted">Member since: <%= member.created_at.strftime("%B %Y") %></small>
                      <br>
                  
                   
                  <%= link_to 'Edit', edit_jboard_path(member) %> | 
                  <%= link_to 'Delete', jboard_path(member), method: :delete %>
                  <hr>
              <% end %>
            <% end %>

          </div>
        </div>

      
    </div> <!-- End Center Column -->

    <div class="col-md-4"><!-- Start Right Column -->
    

      <h1>Place Holder</h1>


    </div><!-- End Right Column -->

  </div> <!-- End Second Row -->
  </div> <!-- End body container Row -->


</div><!-- End Container -->

