
<div id="wrapper"><!-- START BODY CONTAINER -->
  

      <div id="sidebar-wrapper" class="col-md-2">
            <div id="sidebar">
                <ul class="nav list-group">
                    <li>
                        <%= link_to 'Post a Job', new_user_jboard_path(current_user), class: "list-group-item"%>
                    </li>
                    <!-- <li>
                        <%= link_to "Create A Post", new_user_post_path(current_user), class: "list-group-item" %>
                    </li>
                    <li>
                        <%= link_to "Show Messages", user_messages_path(current_user), class: "list-group-item" %>
                    </li> -->
                </ul> 
                <ul class="body-panel">
                  <h4>Job Search</h4>
                 
                </ul>
                <%= render 'layouts/footer'  %>
            </div>
        </div>

        <div id="center-wrapper" class="col-md-6"><!-- START CENTER COLUMN -->
          <div id="center">
            <h3 class="inline"> Search Results 
            <%= link_to 'Search Again', jboards_path, class: "btn btn-warning btn-xs" %>
            </h3>
            <% if @jsearch.search_jboards.empty? %>
                <p>No Records Found</p>
            <% else %> 
                
              <% @jsearch.search_jboards.order("created_at DESC").each do |c| %>
                  <strong><h4 class="jboardTitleOverride"><%= link_to c.title, jboard_path(c) %></h4></strong>
                    <%= c.agency %><br>
                    <%= "#{c.city}, #{c.state}" %><br>

                    <small class="text-muted">Posted <%= time_ago_in_words(c.created_at) %> ago </small>

                  <small class="text-muted">in category: <%= c.category %></small><br>
                  <small><%= link_to 'See Details', jboard_path(c) %></small> 
                  <hr>
              <% end %>
            <% end %>
          </div>
        </div> <!-- END CENTER COLUMN -->

        <div id="right-wrapper" class="col-md-4"> <!-- Begin right column -->
          <div id="sidebar">
           

        <%= link_to(image_tag("300x250.png", :alt => "whatever", :width => 300, :height => 250, :title => "something", class: 'reference'), "http://www.google.com/") %>

        


          </div>
        </div> <!-- END right COLUMN -->

   
  </div><!-- END BODY CONTAINER --> 


