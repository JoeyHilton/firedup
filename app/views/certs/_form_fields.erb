
  <%= bootstrap_form_for [@user, @cert], html: { multipart: true }, remote: using_modal do |f| %>
  <%= render 'devise/shared/error_messages', object: f.object %>
  <%= token_tag(nil) %>
  <%#= f.label :job_company %>
  <%= f.text_field :title %>

  <%#= f.label :job_start_date %>
  <%= f.date_select :start_date %>

  <%#= f.label :job_end_date %>
  <%= f.date_select :expire_date %>

  <%#= f.label :job_description %>
  <%= f.text_area :description %>

  <%= f.file_field :attachment, label: "Add File"%>

  <%= f.submit class: "btn btn-warning"%>
  <% end %>

