
  <%= bootstrap_form_for [@user, @ce], html: { multipart: true }, remote: using_modal do |f| %>
  <%= render 'devise/shared/error_messages', object: f.object %>
  <%= token_tag(nil) %>
  <%#= f.label :job_company %>
  <%= f.text_field :topic %>

  <%#= f.label :job_start_date %>
  <%= f.date_select :date, :start_year=>1950 %>

  <%#= f.label :job_end_date %>
  <%= f.number_field :hours %>

  <%#= f.label :job_description %>
  <%= f.select :classtype, Ce::CLASSTYPE, prompt: "Pick one", label: "CLass Type" %>

  <%#= f.label :job_description %>
  <%= f.text_area :notes %>

  <%= f.file_field :file, label: "Add File"%>

  <%= f.submit "Submit", class: "btn btn-warning"%>
  <% end %>
