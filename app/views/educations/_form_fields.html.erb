<%= form_for [@profile, @education] do |f| %>
  <%= f.label :edu_school %>: 
  <%= f.text_field :edu_school, class: 'form-control' %>

  <%= f.label :edu_subject %> 
  <%= f.text_field :edu_subject, class: 'form-control' %>

  <%= f.label :edu_degree %>
  <%= f.text_field :edu_degree, class: 'form-control' %>

  <%= f.label :edu_description %>
  <%= f.text_area :edu_description, class: 'form-control' %>

  <%= f.label :edu_startdate %> 
  <%= f.date_select :edu_startdate, class: 'form-control'  %>

  <%= f.label :edu_enddate %> 
  <%= f.date_select :edu_enddate, class: 'form-control'  %>

  <%= f.submit %>
<% end %>