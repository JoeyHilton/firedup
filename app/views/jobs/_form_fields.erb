<%= form_for @something do |f| %>
  <%= f.label :job_company %>
  <%= f.text_field :job_company %>

  <%= f.label :start_date %>
  <%= f.date_select :start_date %>

  <%= f.label :end_date %>
  <%= f.date_select :end_date %>

  <%= f.label :job_position %>
  <%= f.text_field :job_position %>

  <%= f.label :job_description %>
  <%= f.text_area :job_description %>

  <%= f.submit %>
<% end %>