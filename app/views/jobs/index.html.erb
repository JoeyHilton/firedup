<h1> Jobs </h1>
<h2><%= link_to 'New Job', new_job_path %></h2>
<% @jobs.each do |job| %>
  <h2><%= job.job_company %></h3>
  <h3><%= job.job_start_date %></h3>
  <h3><%= job.job_end_date %></h3>
  <h4><%= job.job_position %></h4>
  <p><%= job.job_description %></p>
  <h2><%= link_to 'Show Job', job_path(job) %></h2>
  <h2><%= link_to 'Edit Job', edit_job_path(job) %></h2>
  <h2><%= link_to 'Destroy Job', job, method: :delete %></h2>
<% end %>